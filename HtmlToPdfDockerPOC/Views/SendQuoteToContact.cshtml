@model HtmlToPdfDockerPOC.Quote;
<style>
    .none-margin-bottom {
        margin-bottom: 0;
    }

    .none-margin-top {
        margin-top: 0;
    }

    .row {
        display: flex;
        justify-content: space-between;
    }

    .align-left {
        flex: auto;
        text-align: left;
    }

    .align-middle {
        flex: auto;
        text-align: center;
    }

    .align-right {
        flex: auto;
        text-align: right;
    }

    .h3 {
        font-size: 18.72px;
    }

    .bold {
        font-weight: bold;
    }

    .row-space-around {
        display: flex;
        justify-content: space-around;
    }

    .flex {
        display: flex;
    }

    .table {
        width: 100%;
        font-size: 15px;
    }

    .table-border {
        border: 2px solid #3b5c9d;
        border-collapse: collapse;
    }

    .thead {
        background-color: #1f77b7;
        color: #fff;
        text-align: center;
        font-size: 19px;
    }

    .td {
        border: 2px solid #3b5c9d;
        font-size: 17px;
    }

    .text-center {
        text-align: center;
    }

    .expired {
        color: red;
    }

    .h4 {
        font-size: 16px;
    }

    .total {
        color: #fff;
        background-color: #366092;
    }

    .quote-details {
        display: flex;
        flex-direction: column;
        align-items: flex-end;
    }

    .line {
        margin-bottom: 0px;
        color: #3b5c9d;
    }

    .item-under-line {
        text-align: center;
        margin-top: 0px;
    }

    .approval-signature {
        display: flex;
    }

    .approval-signature-item1 {
        flex: 1;
        text-align: right;
    }

    .approval-signature-item2 {
        flex: 2;
    }

    .approval {
        display: flex;
        justify-content: space-around;
    }

    .printed {
        display: flex;
    }

    .printed-item1 {
        flex: 1;
    }

    .printed-item2 {
        flex: 2
    }

    .h2 {
        font-size: 24px;
    }

    .h5 {
        font-size: 13.28px;
    }

    .h6 {
        font-size: 10.72px;
    }

    .dark-blue {
        color: #366092
    }

    .table-striped tbody tr:nth-child(even) {
        background-color: rgb(239, 240, 240);
    }
</style>

